#include<bits/stdc++.h>
using namespace std;

int main(){
    int t;
    cin>>t;
    for(int i=1;i<=t;i++){
        int n=0,nn,p;
        vector<int> arr(5000,0);
        
        cin>>n;
        for(int i=0;i<n;i++){
            int x=0,y=0;
            cin>>x;
            cin>>y;
            for(int i=x;i<=y;i++){
                arr[i]++;
            }
        }
        cout<<"Case #"<<i<<": ";
     cin>>p;
      for(int i=1;i<=p;i++){
          int inx;
          cin>>inx;
          cout<<arr[inx]<<" ";
      }
     cout<<endl;
        
    }
    
}
